{
    "compilerOptions": {
        "target": "ES2018",
        "module": "commonjs",
        "jsx": "react-native",
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "lib": ["dom", "esnext"],
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "noImplicitAny": false
    },
    "exclude": ["./node_modules", "./utils"]
}
